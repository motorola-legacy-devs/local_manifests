<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="lineageos"
           fetch="https://github.com/LineageOS/"
           revision="lineage-18.1" />

  <remote  name="motolegacy"
           fetch="https://github.com/motorola-legacy-devs/"
           revision="lineage-18.1" />

  <remote  name="proton" 
           fetch="https://github.com/ProtonAOSP/"
           revision="rvc" />

  <!-- Additional -->
  <project path="patches" name="android_patches" revision="master" remote="motolegacy" />
  <project path="external/bson" name="android_external_bson" remote="lineageos" />
  <project path="hardware/motorola" name="android_hardware_motorola" remote="lineageos" />
